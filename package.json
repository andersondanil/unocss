{
  "name": "@unocss/monorepo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "pnpm -r --parallel --filter !playground run build",
    "dev": "pnpm -r --parallel --filter !playground run dev",
    "lint": "eslint \"**/*.{ts,vue,mjs}\"",
    "lint:fix": "nr lint -- --fix",
    "play": "npm -C playground run dev",
    "bench": "npm -C bench run bench",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish",
    "test": "jest",
    "test:update": "jest -u"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.9.0",
    "@antfu/ni": "^0.9.3",
    "@iconify-json/carbon": "^1.0.5",
    "@iconify-json/logos": "^1.0.1",
    "@iconify-json/mdi": "^1.0.4",
    "@iconify-json/ph": "^1.0.1",
    "@iconify-json/twemoji": "^1.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@unocss/core": "workspace:*",
    "@unocss/preset-attributify": "workspace:*",
    "@unocss/preset-icons": "workspace:*",
    "@unocss/preset-uno": "workspace:*",
    "@unocss/reset": "workspace:*",
    "@unocss/vite": "workspace:*",
    "@vueuse/core": "^6.5.3",
    "bumpp": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.7.0",
    "esno": "^0.10.1",
    "jest": "^27.2.5",
    "ts-jest": "^27.0.5",
    "tsup": "^5.4.0",
    "typescript": "^4.4.3",
    "unocss": "workspace:*"
  }
}
